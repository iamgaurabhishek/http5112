﻿@model MyPassionProjectW2024n01605783.Models.WorkoutDto


@{
    ViewBag.Title = "View";
}

<a style="color:#ab2020;" href="/Workout/DeleteConfirm/@Model.WorkoutId" class="btn btn-danger">Delete</a>
